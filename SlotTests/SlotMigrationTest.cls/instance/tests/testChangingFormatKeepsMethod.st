testChangingFormatKeepsMethod	aClass := self make: [ :builder | 		builder 			name: self aClassName;			layoutClass: PointerLayout ].	aClass compile: 'a ^self'.	self assert: (aClass methodDictionary includesKey: #a).		anotherClass := self make: [ :builder | 		builder 			name: self aClassName;			layoutClass: VariableLayout .			].	self assert: (anotherClass methodDictionary includesKey: #a).	aClass layout checkIntegrity.	anotherClass layout checkIntegrity.